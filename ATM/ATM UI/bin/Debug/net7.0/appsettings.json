{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "writeTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./LoggedFiles/Log For-.txt",
          "rollingInterval": "Day"
        }
      }
    ]


  },
  "AllowedHosts": "*"
}
